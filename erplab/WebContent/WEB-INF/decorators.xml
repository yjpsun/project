<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/WEB-INF/decorators">
	<!-- Any urls that are excluded will never be decorated by Sitemesh -->
	<excludes>
		<pattern>/styles/*</pattern>
		<pattern>/scripts/*</pattern>
		<pattern>/images/*</pattern>
		<pattern>/dojo/*</pattern>
		<pattern>/struts/*</pattern>
		<pattern>/json/*</pattern>
		<pattern>/mps/report!buildReport.do</pattern>
		<pattern>/mps/bom-report!buildReport.do</pattern>
		<pattern>/mps/bom-report!buildGRReport.do</pattern>
	</excludes>

	<decorator name="main" page="default.jsp">
	    <pattern>*</pattern>
	</decorator>
</decorators>
